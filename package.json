{
  "name": "pdf-rag",
  "version": "1.0.0",
  "description": "PDF RAG Application with Next.js client and Express server",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --names \"SERVER,CLIENT\" --prefix-colors \"blue,green\"",
    "dev:server": "cd server && npm run services:start && npm run dev",
    "dev:client": "cd client && npm run dev",
    "services:start": "cd server && docker-compose up -d",
    "services:stop": "cd server && docker-compose down",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "pdf",
    "rag",
    "nextjs",
    "express",
    "vector-search"
  ],
  "author": "",
  "license": "ISC"
}